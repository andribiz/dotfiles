{
    "title": "Tabs to esc",
    "rules": [
        {
            "description": "Tabs to escape",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "escape"
                        }
                    ],
                }
            ]
        }
    ]
}
